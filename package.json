{
    "name": "themes-falcon-vscode",
    "version": "1.1.0",
    "displayName": "Falcon Relaxing-Eyes Themes",
    "homepage": "https://github.com/panxiaoan/falcon-vscode-themes",
    "description": "A gentle, eye-friendly theme to protect your eyesight for VS Code",
    "publisher": "panxiaoan",
    "license": "MIT",
    "scripts": {
        "build": "node ./scripts/build.js",
        "lint": "node ./scripts/lint.js",
        "package": "vsce package -o themes-falcon-vscode.vsix",
        "vscode:prepublish": "npm run build",
        "vsce-publish": "vsce publish"
    },
    "maintainers": [
        "Xiaoan Pan"
    ],
    "contributors": [
        "Xiaoan Pan"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/panxiaoan/falcon-vscode-themes.git"
    },
    "bugs": {
        "url": "https://github.com/panxiaoan/falcon-vscode-themes/issues"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "falcon",
        "light",
        "theme",
        "color-theme",
        "relaxing-eyes",
        "protect-eyesight",
        "eyesight-protection",
        "celadon",
        "green",
        "pink",
        "yellow",
        "grey"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#E0F2F1",
        "theme": "light"
    },
    "previewImages": [
        "assets/light-green.jpg",
        "assets/light-celadon.jpg",
        "assets/light-pink.jpg"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Falcon Light Celadon",
                "uiTheme": "vs",
                "path": "./theme/light/celadon.json"
            },
            {
                "label": "Falcon Light Green",
                "uiTheme": "vs",
                "path": "./theme/light/green.json"
            },
            {
                "label": "Falcon Light Pink",
                "uiTheme": "vs",
                "path": "./theme/light/pink.json"
            }
            ,
            {
                "label": "Falcon Light Yellow",
                "uiTheme": "vs",
                "path": "./theme/light/yellow.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0",
        "prettier": "^3.0.0",
        "tinycolor2": "^1.6.0",
        "vsce": "^2.15.0"
    }
}
